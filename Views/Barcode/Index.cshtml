<div class="wrapper">
    <div class="panel panel-hash">
        <div class="panel-heading">
            <div class="row">
                <div class="col-md-5">
                </div>
                <div class="col-md-7 pull-right">
                    <h5 class="pull-right"><b>&nbsp;@Resource.AssetBarcodeList</b></h5>
                </div>
            </div>
        </div>

        <div class="panel-body scroll panel-height">
            <table id="tblBarcode" class="CustomBlueTable" style="width:100%">
                <thead>
                    <tr>
                        <th>@Resource.Id</th>
                        <th>@Resource.Name</th>
                        <th>@Resource.Barcode</th>
                        <th></th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>

    <div class="row" id="divSingleBarcodeDIV">
        <div class="col-sm-3">
            <span><img src="" alt="" id="imgSingleBarcode" class="" /></span><br />
            <sapn id="Metadata"></sapn>
        </div>
        <div class="col-sm-9">
        </div>
    </div>
</div>
@{
    var title = Resource.AssetBarcodeList;
    var tml = Resource.TML;
    var print = Resource.Print;
    var assetdet = Resource.AssetDetails;
    var search = Resource.Search;
}
<div id="resourcetitle" data-title="@title"></div>
<div id="resourcetml" data-tml="@tml"></div>
<div id="resourceprint" data-print="@print"></div>
<div id="resourcesearch" data-search="@search"></div>
<div id="resourceassetdet" data-assetdet="@assetdet"></div>
<style>
    .panel-heading {
        padding: 0px 0px 0px 5px;
    }

    .panel-body {
        padding: 5px;
    }

    .h4 {
        margin-top: 1px;
        margin-bottom: 1px;
    }
</style>

<script src="~/js/Barcode/Barcode_Datatable.js"></script>
<script>
    $(document).ready(function () {
        var search = $('#resourcesearch').data('search');
        DataTableCustomSearchBox('450px', search);
    });
</script>