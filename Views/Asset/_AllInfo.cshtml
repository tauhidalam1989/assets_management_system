@model AMS.Models.AssetViewModel.AssetCRUDViewModel

<div class="card card-primary card-outline card-tabs">
    <div class="card-header p-0 pt-1 border-bottom-0">
        <ul class="nav nav-tabs" id="custom-tabs-three-tab" role="tablist">
            <li class="nav-item">
                <a class="nav-link active" id="Tab01" data-toggle="pill" href="#divTab01" role="tab"
                    aria-controls="Tab01" aria-selected="true">@Resource.AssetInfo</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="Tab02" data-toggle="pill" href="#divTab02" role="tab" aria-controls="Tab02"
                    aria-selected="true">@Resource.AssigneeInfo</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="Tab03" data-toggle="pill" href="#divTab03" role="tab" aria-controls="Tab03"
                   aria-selected="true">@Resource.AssetHistory</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="Tab04" data-toggle="pill" href="#divTab04" role="tab" aria-controls="Tab04"
                    aria-selected="true">@Resource.CommentHistory</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="Tab05" data-toggle="pill" href="#divTab05" role="tab" aria-controls="Tab05"
                   aria-selected="true">@Resource.RequestLog</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="Tab06" data-toggle="pill" href="#divTab06" role="tab" aria-controls="Tab06"
                   aria-selected="true">@Resource.IssueLog</a>
            </li>
        </ul>
    </div>

    <div class="card-body">
        <div class="tab-content" id="tabContent">
            <div class="tab-pane fade show active" id="divTab01" role="tabpanel" aria-labelledby="divTab01">
                <partial name="_Details.cshtml" />
            </div>

            <div class="tab-pane fade" id="divTab02" role="tabpanel" aria-labelledby="divTab02Tab">
                @if (@Model.UserProfileCRUDViewModel != null)
                {
                    <partial name="_AllocateAssetDisplay" />
                }
                else
                {
                    <h1>@Resource.NotAssigned</h1>
                }
            </div>
            <div class="tab-pane fade show" id="divTab03" role="tabpanel" aria-labelledby="divTab03">
                <partial name="_AssetHistory" />
            </div>
            <div class="tab-pane fade" id="divTab04" role="tabpanel" aria-labelledby="divTab04">
                <partial name="_CommentHistoryInView" />
            </div>
            <div class="tab-pane fade" id="divTab05" role="tabpanel" aria-labelledby="divTab05">
                <partial name="~/Views/AssetRequest/_AssetRequestLog.cshtml" model="Model" />
            </div>
            <div class="tab-pane fade" id="divTab06" role="tabpanel" aria-labelledby="divTab06">
                <partial name="~/Views/AssetIssue/_AssetIssueLog.cshtml" model="Model" />
            </div>
        </div>
    </div>
</div>



