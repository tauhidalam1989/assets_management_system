@model AMS.Models.AssetViewModel.AssetCRUDViewModel
<h5>@Resource.Asset_Id: @Model.Id, Name: @Model.Name</h5>
<table class="CustomBlueTable">
    <thead>
        <tr>
            <th>@Resource.Id</th>
            <th>@Resource.AssignEmployee</th>
            <th>@Resource.Action</th>
            <th>@Resource.Note</th>
            <th>@Resource.CreatedDate</th>
        </tr>
    </thead>
    <tbody>
        @{ foreach (var item in Model.listAssetHistoryCRUDViewModel)
            {
<tr>
    <td>@item.Id</td>
    <td>@item.AssignEmployeeDisplay</td>
    <td>@item.Action</td>
    <td>@item.Note</td>
    <td>@String.Format("{0:f}", item.CreatedDate)</td>
</tr>
 } }
    </tbody>
</table>


