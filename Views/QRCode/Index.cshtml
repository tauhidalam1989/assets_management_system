<div class="wrapper">
    <div class="panel panel-hash">
        <div class="panel-heading">
            <div class="row">
                <div class="col-md-5">
                </div>
                <div class="col-md-7 pull-right">
                    <h5 class="pull-right"><b>&nbsp;@Resource.AssetQRCodeList</b></h5>
                </div>
            </div>
        </div>

        <div class="panel-body scroll panel-height">
            <table id="tblQRCode" class="CustomBlueTable" style="width:100%">
                <thead>
                    <tr>
                        <th>@Resource.Id</th>
                        <th>@Resource.Name</th>
                        <th>@Resource.QRCode</th>
                        <th></th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>

    <div class="row" id="divSingleBarcodeDIV">
        <div class="col-sm-3">
            <span><img src="" alt="" id="imgSingleBarcode" class="" /></span><br />
            <sapn id="Metadata"></sapn>
        </div>
        <div class="col-sm-9">
        </div>
    </div>
</div>

<style>
    .panel-heading {
        padding: 0px 0px 0px 5px;
    }

    .panel-body {
        padding: 5px;
    }

    .h4 {
        margin-top: 1px;
        margin-bottom: 1px;
    }
</style>

<script src="~/js/QRCode/QRCode_Datatable.js"></script>
<script>
    $(document).ready(function () {
        DataTableCustomSearchBox('450px', 'Search Item...');
    });
</script>
