<div class="wrapper">
    <div class="panel panel-hash">
        <div class="panel-heading">
            <div class="row">
                <div class="col-md-5">
                    <a class="fa fa-plus-square fa-2x" onclick="AddEdit(0,'@Resource.EditDesignation','@Resource.AddDesignation')"></a>
                </div>
                <div class="col-md-7 pull-right">
                    <h5 class="pull-right"><b>&nbsp;@Resource.DesignationList</b></h5>
                </div>
            </div>
        </div>

        <div class="panel-body scroll panel-height">
            <table id="tblDesignation" class="CustomBlueTable" style="width:100%">
                <thead>
                    <tr>
                        <th>@Resource.Id</th>
                        <th>@Resource.Name</th>
                        <th>@Resource.Description</th>
                        <th>@Resource.CreatedDate</th>

                        <th></th>
                        <th></th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
    <partial name="~/Views/UIComponents/_bootstrapAlert.cshtml" />
</div>
@{
    var title = Resource.Designation;
    var adddesg = Resource.AddDesignation;
    var editdesg = Resource.EditDesignation;
    var yes = Resource.Yes;
    var edit = Resource.Edit;
    var del = Resource.Delete;
    var desgdet = Resource.DesignationDetails;
    var msgdel = Resource.Msg_Delete;
    var delmsg = Resource.MSG_DesigDelSuccess;
}
<div id="resourcetitle" data-title="@title"></div>
<div id="resourcedesgdet" data-desgdet="@desgdet"></div>
<div id="resourceyes" data-yes="@yes"></div>
<div id="resourceedit" data-edit="@edit"></div>
<div id="resourcedel" data-del="@del"></div>
<div id="resourceadddesg" data-adddesg="@adddesg"></div>
<div id="resourceeditdesg" data-editdesg="@editdesg"></div>
<div id="resourcemsgdel" data-msgdel="@msgdel"></div>
<div id="resourcedelmsg" data-delmsg="@delmsg"></div>
<style>
    .panel-heading {
        padding: 0px 0px 0px 5px;
    }

    .panel-body {
        padding: 5px;
    }

    .h4 {
        margin-top: 1px;
        margin-bottom: 1px;
    }
</style>

<script src="~/js/Designation/Designation_CRUD.js"></script>
<script src="~/js/Designation/Designation_Datatable.js"></script>

