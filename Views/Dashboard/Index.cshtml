@{
    ViewData["Title"] = "Dashboard";
}
<div class="wrapper">
    @{
        var _IsInRole = User.IsInRole("Admin");
        if (_IsInRole)
        {
            <partial name="_AssetSummary" />
            <partial name="_RecentRegisteredAsset" />

            <partial name="_PieChart" />
            <hr />
            <partial name="_UserSummary" />
            <partial name="_RecentRegisteredUser" />
        }
    }
</div>

